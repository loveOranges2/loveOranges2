<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN" 
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="edu.gatech.i3l.jpa.model.omop.Observation" table="observation">
		<id name="id" column="observation_id" type="long">
			<generator class="hilo" />
		</id>
		<many-to-one name="person" class="edu.gatech.i3l.jpa.model.omop.Person" not-null="true" column="person_id"/>
		<many-to-one name="observationConcept" class="edu.gatech.i3l.jpa.model.omop.Concept" not-null="true" column="observation_concept_id"/>
		<property name="date" column="observation_date" not-null="true"/>
		<!-- <property name="time" column="observation_time" type="org.joda.time.contrib.hibernate.PersistentLocalTimeAsTime"/> -->
		<property name="valueAsString" column="value_as_string"/>
		<property name="valueAsNumber" column="value_as_number" type="java.math.BigDecimal"/>
		<many-to-one name="unit" class="edu.gatech.i3l.jpa.model.omop.Concept" column="unit_concept_id"/>
		<property name="rangeHigh" column="range_high" type="java.math.BigDecimal"/>
		<property name="rangeLow" column="range_low" type="java.math.BigDecimal"/>
		<many-to-one name="observationValueAsConcept" class="edu.gatech.i3l.jpa.model.omop.Concept" column="value_as_concept_id"/>
		<many-to-one name="type" class="edu.gatech.i3l.jpa.model.omop.Concept" column="observation_type_concept_id" not-null="true"/>
		<many-to-one name="provider" class="edu.gatech.i3l.jpa.model.omop.Provider" column="associated_provider_id"/>
		<many-to-one name="relevantCondition" column="relevant_condition_concept_id" class="edu.gatech.i3l.jpa.model.omop.Concept"/>
		<property name="sourceValue" column="observation_source_value" type="string"/>
		<property name="unitsSourceValue" column="units_source_value" type="string"/>
		<many-to-one name="visitOccurrence" column="visit_occurence_id" class="edu.gatech.i3l.jpa.model.omop.VisitOccurrence" not-null="true"/>
	</class>
</hibernate-mapping>