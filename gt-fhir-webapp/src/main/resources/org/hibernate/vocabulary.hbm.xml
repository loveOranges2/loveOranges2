<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN" 
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="edu.gatech.i3l.jpa.model.omop.Vocabulary" table="vocabulary">
		<id name="id" column="vocabulary_id" type="long">
			<generator class="identity"/>
		</id>
		<property name="name" column="vocabulary_name" type="string"/>
	</class>
</hibernate-mapping>